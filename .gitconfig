[user]
	email = kino811@gmail.com
	name = kino
[core]
	editor = emacsclient
	crlf = input
[merge]
	tool = emerge -q -nw
[push]
	default = matching
[diff]
	tool = ediff -q -nw
[alias]
	st = status
	logl = log --oneline --decorate --graph --all
[credential]
	helper = cache
[color]
	ui = always
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
